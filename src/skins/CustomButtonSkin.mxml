<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
			 xmlns:fb="http://ns.adobe.com/flashbuilder/2009" minWidth="21" minHeight="21" alpha.disabled="0.5">
	<fx:Metadata>
		<![CDATA[
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<s:Rect id="fill" left="0" right="0" top="0" bottom="0" >
		<s:fill>
			<s:SolidColor color="0xffd700" color.over="0xffdf33" color.down="0xffb500" color.disabled="0xd3d3d3" />
		</s:fill>
	</s:Rect>
	
	<s:Rect id="border" left="0" right="0" top="0" bottom="0" alpha="0" />
	
	<s:Label id="labelDisplay" color="#4b4b4b" fontWeight="bold" color.disabled="#ffffff"
			 textAlign="center" maxDisplayedLines="1"
			 horizontalCenter="0" verticalCenter="1" verticalAlign="middle"
			 left="15" right="15" top="7" bottom="7">
	</s:Label>
	
</s:SparkSkin>
